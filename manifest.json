{
  "name": "Work and rest",
  "description": "A timer to alert users when it is time for a break",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "extension.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["public/content.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "scripting"]
}
